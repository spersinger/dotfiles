def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "ctrl-u"
    page_up
#push_characters "kkkkkkkkkkkkkkk"
def_key "ctrl-d"
    page_down
#push_characters "jjjjjjjjjjjjjjj"

def_key "h"
    previous_column

def_key "l"
    next_column

def_key "h"
    master_screen

def_key "l"
    slave_screen

def_key "."
    show_lyrics

